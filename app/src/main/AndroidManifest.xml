<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartandsafeautomatedhome">


    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/light_on"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />



                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".bedroom1"></activity>
        <activity android:name=".bedroom2"></activity>
        <activity android:name=".Tvlounge"></activity>
        <activity android:name=".Kitchen"></activity>
        <activity android:name=".Security"></activity>
        <activity android:name=".SchedulingActivity"></activity>

    </application>

</manifest>